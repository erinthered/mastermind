;; describe mystery codes 1, 3, 4 by number of colors in each code 

(defun my-color-counter (code)
  (color-counter *Mastermind* code))

(defun num-colors-summary (codes)
  (let ((result (mapcar #'(lambda (x) (count 0 x)) (mapcar #'my-color-counter codes))))
    (setf result (mapcar #'(lambda (x) (- 5 x)) result))
    (format t "~%Codes with 1 color: ~a" (count 1 result))
    (format t "~%Codes with 2 color: ~a" (count 2 result))
    (format t "~%Codes with 3 color: ~a" (count 3 result))
    (format t "~%Codes with 4 color: ~a" (count 4 result))
    (format t "~%Codes with 5 color: ~a" (count 5 result))))

